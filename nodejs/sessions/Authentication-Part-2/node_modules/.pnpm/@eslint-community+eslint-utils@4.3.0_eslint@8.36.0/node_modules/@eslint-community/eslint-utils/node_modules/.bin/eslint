#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/eslint@8.36.0/node_modules/eslint/bin/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/eslint@8.36.0/node_modules/eslint/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/eslint@8.36.0/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules:/home/workspace/nodejs/sessions/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/@eslint-community+eslint-utils@4.3.0_eslint@8.36.0/node_modules/@eslint-community/eslint-utils/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/@eslint-community+eslint-utils@4.3.0_eslint@8.36.0/node_modules/@eslint-community/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/@eslint-community+eslint-utils@4.3.0_eslint@8.36.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/eslint@8.36.0/node_modules/eslint/bin/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/eslint@8.36.0/node_modules/eslint/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/eslint@8.36.0/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules:/home/workspace/nodejs/sessions/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/@eslint-community+eslint-utils@4.3.0_eslint@8.36.0/node_modules/@eslint-community/eslint-utils/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/@eslint-community+eslint-utils@4.3.0_eslint@8.36.0/node_modules/@eslint-community/node_modules:/home/workspace/nodejs/sessions/Authentication-Part-2/node_modules/.pnpm/@eslint-community+eslint-utils@4.3.0_eslint@8.36.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@8.36.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@8.36.0/node_modules/eslint/bin/eslint.js" "$@"
fi
